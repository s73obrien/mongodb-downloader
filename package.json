{
  "name": "mongodb-downloader",
  "version": "1.0.0",
  "description": "Downloads mongodb prebuilt packages from fastdl.mongodb.org",
  "main": "lib/index.js",
  "bin": {
    "mongodb-downloader": "lib/cli.js"
  },
  "scripts": {
    "lint": "eslint lib test",
    "unit-tests": "tape test/*.js | tnyan",
    "test": "npm run unit-tests && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s73obrien/mongodb-downloader.git"
  },
  "authors": [
    "Sean O'Brien <s73obrien@hotmail.com>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/s73obrien/mongodb-downloader/issues"
  },
  "homepage": "https://github.com/s73obrien/mongodb-downloader#readme",
  "dependencies": {
    "debug": "^3.0.0",
    "env-paths": "^1.0.0",
    "fs-extra": "^4.0.1",
    "minimist": "^1.2.0",
    "nugget": "^2.0.1",
    "path-exists": "^3.0.0",
    "rc": "^1.2.1",
    "semver": "^5.4.1",
    "sumchecker": "^2.0.2"
  },
  "devDependencies": {
    "dotenv": "^4.0.0",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "faucet": "0.0.1",
    "tap-nyan": "^1.1.0",
    "tape": "^4.8.0",
    "temp": "^0.8.3"
  },
  "eslintConfig": {
    "extends": "standard",
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "strict": [
        "error"
      ],
      "prefer-const": [
        "error"
      ]
    }
  },
  "keywords": [],
  "engines": {
    "node": ">= 4.0"
  }
}
